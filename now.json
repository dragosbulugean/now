{
  "version": 2,
  "builds": [
    { "src": "api/api.js", "use": "@now/node-server" },
    { "src": "app/next.config.js", "use": "@now/next" },
    { "src": "app/static/*", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/app/$1" }
  ]
}
